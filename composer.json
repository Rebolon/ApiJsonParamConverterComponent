{
    "name": "rebolon/api-json-param-converter",
    "type": "library",
    "description": "A component to use with your project using ApiPlatform to allow easy custom routes creation.",
    "keywords": ["REST", "API", "JSON"],
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "api-platform/core": "^2.2.4",
        "doctrine/orm": "^2.6.1",
        "sensio/framework-extra-bundle": "^5.1.6",
        "symfony/http-foundation": "^4.0.6",
        "symfony/property-access": "^4.0.6",
        "symfony/property-info": "^4.1",
        "symfony/serializer": "^4.0.6",
        "symfony/validator": "^4.0.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "roave/security-advisories": "dev-master",
        "sensiolabs/security-checker": "^4.1",
        "friendsofphp/php-cs-fixer": "^2.10",
        "squizlabs/PHP_CodeSniffer": "^3.2",
        "doctrine/common": "^2.8.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Rebolon\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rebolon\\Tests\\": "tests/"
        }
    },
    "suggest": {
    }
}
